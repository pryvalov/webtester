<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-configuration SYSTEM "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory >
  		<property name="hibernate.dialect">org.hibernate.dialect.PostgreSQL82Dialect</property>
  		<property name="hibernate.show.sql">false</property>
  
        <mapping class="ua.pri.ent.Account"/>
        <mapping class="ua.pri.ent.Role"/>
        <mapping class="ua.pri.ent.AccountVerification"/>
        <mapping class="ua.pri.ent.Test"/>
        <mapping class="ua.pri.ent.Question"/>
        <mapping class="ua.pri.ent.Answer"/>
        <mapping class="ua.pri.ent.TestResult"/>
    </session-factory>
</hibernate-configuration>